<!-- Modal -->
<div
    class="modal fade"
    id="uploadModal"
    tabindex="-1"
    role="dialog"
    aria-labelledby="uploadDeviceTitle"
    aria-hidden="true"
>
    <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
        <div class="modal-content">
            {{ form_start(form) }}
            <div class="modal-header">
                <h5
                    class="modal-title"
                    id="uploadModalTitle"
                >
                    Upload Excel
                </h5>
                <button
                    type="button"
                    class="close"
                    data-dismiss="modal"
                    aria-label="Close"
                >
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <input
                    type="file"
                    name="file"
                    id="devices_excel"
                    class="btn btn-success fileinput-button"
                    multiple="multiple"
                />
                <div id="progress">
                    <div class="bar" style="width: 0%;"></div>
                </div>
            </div>
            <div class="modal-footer"></div>
            {{ form_end(form) }}
        </div>
    </div>
</div>
<!-- end Modal -->
