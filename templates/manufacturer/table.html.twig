<div class="table-responsive-md">
    <table class="table">
        <thead class="thead-light">
            <tr>
                <th>Band</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            {% for item in manufacturers %}
            <tr>
                <td class=""> {{ item.getName() }} </td>
                <td>
                    <a class="btn btn-default btn-lg"  href="
                        {{ path(
                            'edit_manufacturer',
                            {'manufacturer':item.getId()}
                        ) }}
                    ">
                        <span
                            class="glyphicon glyphicon-pencil"
                            aria-hidden="true"
                        ></span> Edit
                    </a>

                    <a
                        class="btn btn-default btn-lg "
                        aria-data="item.getId()"
                    >
                        <span
                            class="glyphicon glyphicon-pencil"
                            aria-hidden="true"
                        ></span> Delete
                    </a>

                </td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
<div>
